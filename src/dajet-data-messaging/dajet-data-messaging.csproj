<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>DaJet.Data.Messaging</RootNamespace>
    <AssemblyName>DaJet.Data.Messaging</AssemblyName>
    <Copyright>Zhichkin Â© 2021</Copyright>
    <Authors>Zhichkin</Authors>
    <PackageId>DaJet.Data.Messaging</PackageId>
    <Product>DaJet.Data.Messaging</Product>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>1C:Enterprise 8 database adapter for messaging.
Microsoft SQL Server and PostgreSQL are supported.</Description>
    <PackageProjectUrl>https://github.com/zhichkin/dajet-data-messaging</PackageProjectUrl>
